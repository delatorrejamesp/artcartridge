<%if user_signed_in?%>
<%hightlight_vote = current_user.voted_up_on?(@photo) ? "hightlighted_vote" : "unhightlighted_vote"%>
<%end%>

<%=link_to dislike_photo_path(@photo), method: :put, class: 'btn btn-link btn-sm', remote: true do %>
 <span class="fa fa-thumbs-o-up"></span>
<%end%>
<span><%=pluralize(@photo.get_upvotes.size, "like", "likes") if @photo.get_upvotes.size > 0%></span>
