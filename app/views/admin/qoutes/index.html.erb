<p id="notice"><%= notice %></p>

<h1>Admin Qoutes</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th>Enable</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_qoutes.each do |admin_qoute| %>
      <tr>
        <td><%= admin_qoute.description %></td>
        <td><%= admin_qoute.enable %></td>
        <td><%= link_to 'Show', admin_qoute %></td>
        <td><%= link_to 'Edit', edit_admin_qoute_path(admin_qoute) %></td>
        <td><%= link_to 'Destroy', admin_qoute, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin Qoute', new_admin_qoute_path %>
