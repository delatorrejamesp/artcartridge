
<div class="col-md-6" data-target="expandable" data-url="<%=@photo.image.url%>", data-url-med="<%=@photo.image.url(:medium)%>">
      <center>
      <%=image_tag @photo.image.url(:medium), style: "width:100%; hieght:100%;"%>
      <ul class="list-unstyled list-inline">
      <li><span class="fa fa-thumbs-o-up"></span> 2k likes</li>
      <li><span class="fa fa-eye"></span> 1k views</li>
    </ul>
    </center>
</div>
<div class="col-md-6">
  <div class="author-area">
      <%=image_tag @photo.user.avatar.url(:medium), size: "25x25", class: "img-circle" %>
      <%=link_to @photo.user.full_name,  community_page_url(subdomain: "#{@photo.user.slug}") %>
      on
      <%=@photo.created_at.strftime("%B %-d, %Y")%>
  </div>
  <div class="title-description-area" style="">
    <h3><%=@photo.title%></h3>
    <p><%=@photo.description%></p>
  </div>
  <div class="social-icons-area row">
      <div class="col-sm-3 text-center">
            <i class="fa fa-facebook" aria-hidden="true"></i>
      </div>
      <div class="col-sm-3 text-center">
            <i class="fa fa-google-plus" aria-hidden="true"></i>
      </div>
      <div class="col-sm-3 text-center">
          <i class="fa fa-twitter" aria-hidden="true"></i>
      </div>
      <div class="col-sm-3 text-center">
            <i class="fa fa-tumblr" aria-hidden="true"></i>
      </div>
  </div>
  <div class="art-analysis-area">
  <h3>Art Analysis</h3>
      <dl class="dl-horizontal">
        <%if !@photo.medium.blank?%>
          <dt>Medium</dt>
          <dd><%=@photo.medium%></dd>
        <%end%>
        <%if !@photo.gene.blank?%>
          <dt>Gene</dt>
          <dd><%=@photo.gene%></dd>
        <%end%>
        <%if !@photo.subject_matter.blank?%>
          <dt>Subject</dt>
          <dd><%=@photo.subject_matter%></dd>
        <%end%>
      </dl>

  </div>
  <div class="art-analysis-area">
    <h3>Tools</h3>
  </div>
  <div class="comments-area">
    <center>
    <iframe src="<%=comments_photo_url(@photo, modal_mode: true)%>" style="width:100%;overflow:hidden;min-height:200px; max-height:600px;" frameBorder="0" height="600">

    </iframe>
  </center>
  </div>

</div>
