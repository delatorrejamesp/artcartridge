<%= form_for(:user, :url => user_session_path) do |f| %>
<li>
    <div class="control-group">
        <%= f.label :email, :class => "control-label" %>
        <div class="controls">
            <%= f.text_field :email, class: "form-control", value: "" %>
        </div>
    </div>
</li>
<li>
    <div class="control-group">
        <%= f.label :password, :class => "control-label" %>
        <div class="controls">
            <%= f.password_field :password, class: "form-control" %>
        </div>
    </div>
</li>
<li>
    <div class="control-group">
      <br>
        <div class="controls">
            <%= f.check_box :remember_me %>
            <%= f.label :remember_me, :class => "control-label" %>

        </div>
    </div>
</li>
<li>
    <%= f.submit "Sign in", :class => "btn btn-primary form-control" %>

</li>
<% end %>
<ul class="list-unstyled">
<li>

    <%=button_to "Sign up", new_user_registration_url(subdomain: false), method: :get, :class => "btn btn-primary form-control",style: "margin-top: 10px;" %>

</li>
</ul>
