<div id="carousel-qoutes" class="carousel slide carousel-fade" data-ride="carousel">
  <ol class="carousel-indicators">
    <%@setting.qoutes_random.each_with_index  do |qoute,index|%>
            <li data-target="#carousel-qoutes" data-slide-to="<%=index%>" class="<%='active' if index == 0 %>"></li>
    <%end%>
  </ol>
  <!-- Wrapper for slides -->
  <div class="carousel-inner" role="listbox"  data-render="home qoute">
    <%#@setting.qoutes_random.each_with_index do |qoute,index|%>
    <%=render partial: "home/qoute", collection: @setting.qoutes_random, as: :qoute %>

    <%#end%>
  </div>
  <a class="left carousel-control" href="#carousel-qoutes" role="button" data-slide="prev">
    <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
    <span class="sr-only">Previous</span>
  </a>
  <a class="right carousel-control" href="#carousel-qoutes" role="button" data-slide="next">
    <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
    <span class="sr-only">Next</span>
  </a>
</div>
<%=javascript_tag do %>
$('#carousel-qoutes').carousel(
  {
   interval: 5000,
   pause: false }
   );
<%end%>
